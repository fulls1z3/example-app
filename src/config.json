{
    "system": {
        "applicationName": "@nglibs example app",
        "applicationUrl": "http://localhost:4200"
    },
    "seo": {
        "defaultPageTitle": "showcasing the @nglibs utilities",
        "pageTitlePositioning": 10,
        "pageTitleSeparator": " | ",
        "defaultMetaDescription": "@nglibs provides a cool, super set of Angular utilities"
    },
    "i18n": {
        "defaultLanguage": {
            "code": "en",
            "name": "English",
            "culture": "en-US"
        },
        "availableLanguages": [
            {
                "code": "en",
                "name": "English",
                "culture": "en-US"
            },
            {
                "code": "tr",
                "name": "Türkçe",
                "culture": "tr-TR"
            }
        ],
        "autoDetectLanguage": true,
        "useLocalizedRoutes": true
    },
    "routes": {
        "en": {
            "ROOT.ABOUT": "about",
            "ROOT.ABOUT.US": "us",
            "ROOT.ABOUT.BANANA": "banana",
            "ROOT.ABOUT.APPLE": "apple",
            "ROOT.ABOUT.APPLE.PEAR": "pear",
            "CHANGE_LANGUAGE": "change-language"
        },
        "tr": {
            "ROOT.ABOUT": "hakkinda",
            "ROOT.ABOUT.US": "biz",
            "ROOT.ABOUT.BANANA": "muz",
            "ROOT.ABOUT.APPLE": "elma",
            "ROOT.ABOUT.APPLE.PEAR": "armut",
            "CHANGE_LANGUAGE": "dil-secimi"
        }
    }
}